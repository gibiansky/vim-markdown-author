{
  "name": "vim-markdown-author",
  "version": "1.0.0",
  "description": "An application for editing Markdown documents with Vim.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/server.js --exec babel-node --presets es2016,stage-2 src/server.js",
    "compile": "babel -d lib/ src/client/ && browserify lib/index.js -o static/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/gibiansky/vim-markdown-author.git"
  },
  "keywords": [
    "vim",
    "markdown"
  ],
  "author": "Andrew Gibiansky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gibiansky/vim-markdown-author/issues"
  },
  "homepage": "https://github.com/gibiansky/vim-markdown-author#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "browserify": "^13.0.0"
  },
  "dependencies": {
    "finalhandler": "^0.4.1",
    "highlight.js": "^9.3.0",
    "jquery": "^2.2.3",
    "mathjax": "^2.6.1",
    "nodemon": "^1.9.1",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "remarkable": "^1.6.2",
    "serve-static": "^1.10.2",
    "socket.io": "^1.4.5",
    "socket.io-client": "^1.4.5"
  }
}
